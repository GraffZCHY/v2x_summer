#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'basic-subscriber==0.0.0','console_scripts','basic_subscriber'
__requires__ = 'basic-subscriber==0.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('basic-subscriber==0.0.0', 'console_scripts', 'basic_subscriber')()
    )
